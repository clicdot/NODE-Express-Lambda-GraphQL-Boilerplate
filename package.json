{
  "name": "Express-Lambda-Boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "./src/lambda.js",
  "scripts": {
    "lambda": "npm run lambda:deploy",
    "lambda:deploy": "claudia create --handler lambda.handler --deploy-proxy-api --region us-east-1",
    "lambda:update": "claudia update",
    "lambda:pack": "claudia pack",
    "lint": "semistandard | snazzy",
    "start": "NODE_ENV=dev nodemon -L -w ./src ./src/server.js",
    "test": "npm run lint && npm run test:unit",
    "test:coverage": "npm run test:unit -- --cov --coverage-report=text --coverage-report=html",
    "test:load": "artillery run ./test-load/root.v1.yml",
    "test:unit": "tap ./test/**/*.test.js"
  },
  "keywords": [],
  "author": "Philip Choi",
  "license": "ISC",
  "dependencies": {
    "app-root-path": "^2.2.1",
    "aws-serverless-express": "^3.3.6",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.3",
    "express-graphql": "^0.9.0",
    "express-jsonschema": "^1.1.6",
    "express-mung": "^0.5.1",
    "express-res-hook": "^1.1.0",
    "fs": "0.0.1-security",
    "graphql": "^14.4.2",
    "graphql-tools": "^4.0.5",
    "helmet": "^3.18.0",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.17.1",
    "path": "^0.12.7",
    "swagger-jsdoc": "^3.2.9",
    "swagger-ui-express": "^4.0.5",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "artillery": "^1.6.0-28",
    "claudia": "^5.5.0",
    "nock": "^10.0.6",
    "nodemon": "^1.19.1",
    "pre-commit": "^1.2.2",
    "semistandard": "^13.0.1",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1",
    "supertest": "^4.0.2",
    "tap": "^14.1.11"
  }
}
